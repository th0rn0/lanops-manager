{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "7.*",
        "laravel/framework": "^6.0",
        "laravelcollective/html": "^6.2.0",
        "webonyx/graphql-php": "dev-master",
        "invisnik/laravel-steam-auth": "^4.3.0",
        "laravel/socialite": "^5.0.1",
        "ignited/laravel-omnipay": "^3.3.0",
        "doctrine/dbal": "2.4",
        "simplesoftwareio/simple-qrcode": "~1",
        "guzzlehttp/guzzle": "6.5.5",
        "lanops/challonge-php": "1.0.9",
        "ipunkt/laravel-analytics": "^4.0.0",
        "intervention/image": "^2.5.1",
        "facebook/graph-sdk": "^5.7",
        "fideloper/proxy": "^4.4",
        "laravel/tinker": "^2.4.2",
        "leafo/scssphp": "^0.7.7",
        "omnipay/paypal": "^3.0.2",
        "cviebrock/eloquent-sluggable": "^6.0.3",
        "barryvdh/laravel-debugbar": "^3.5.1",
        "omnipay/stripe": "^3.1",
        "stripe/stripe-php": "^7.14",
        "symfony/routing": "4.4.9",
        "sirprize/postal-code-validator": "^1.3.6",
        "artesaos/seotools": "^0.19.0",
        "weblagence/laravel-facebook-pixel": "^1.0",
        "spatie/laravel-cookie-consent": "^2.12",
        "xpaw/php-source-query-class": "^1.0"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.4.2",
        "phpunit/phpunit": "^9.4.0",
        "symfony/css-selector": "^5.1.6",
        "symfony/dom-crawler": "^5.1.6",
        "orangehill/iseed": "^2.6.4",
        "squizlabs/php_codesniffer": "^3.5.6",
        "filp/whoops": "^2.7.3"
    },
    "autoload": {
        "classmap": [
            "database",
            "tests/TestCase.php"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": []
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "pre-update-cmd": [
            "php artisan clear-compiled"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "post-update-cmd": [
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}