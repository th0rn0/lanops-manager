{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.2.5|^8.0",
        "laravel/framework": "^6.0",
        "laravelcollective/html": "^6.0",
        "webonyx/graphql-php": "dev-master",
        "invisnik/laravel-steam-auth": "^4.4",
        "laravel/socialite": "^4.0",
        "ignited/laravel-omnipay": "3.1.1",
        "doctrine/dbal": "2.4",
        "simplesoftwareio/simple-qrcode": "~1",
        "guzzlehttp/guzzle": "~5.3|~6.0",
        "intervention/image": "^2.3",
        "facebook/graph-sdk": "^5.7",
        "fideloper/proxy": "^4.0",
        "laravel/tinker": "^1.0",
        "leafo/scssphp": "^0.7.7",
        "omnipay/paypal": "^3.0",
        "cviebrock/eloquent-sluggable": "^6",
        "barryvdh/laravel-debugbar": "^3.2",
        "omnipay/stripe": "^3.1",
        "stripe/stripe-php": "^7.14",
        "sirprize/postal-code-validator": "^1.3",
        "weblagence/laravel-facebook-pixel": "^1.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "^7.0",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*",
        "orangehill/iseed": "2.6.*",
        "squizlabs/php_codesniffer": "3.*",
        "filp/whoops": "~2.0"
    },
    "autoload": {
        "classmap": [
            "database",
            "tests/TestCase.php"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [

        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "pre-update-cmd": [
            "php artisan clear-compiled"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "post-update-cmd": [
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
